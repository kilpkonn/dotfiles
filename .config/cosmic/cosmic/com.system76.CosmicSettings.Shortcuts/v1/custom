{
    (
        modifiers: [
            Super,
            Shift,
        ],
        key: "s",
        description: Some("Flameshot"),
    ): Spawn("XDG_CURRENT_DESKTOP=gnome flameshot gui"),
    (
        modifiers: [
            Super,
            Shift,
        ],
        key: "Return",
        description: Some("Alacritty"),
    ): Spawn("alacritty"),
    (
        modifiers: [
            Super,
            Shift,
        ],
        key: "c",
    ): Close,
}